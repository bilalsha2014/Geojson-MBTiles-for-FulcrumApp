@font_reg: "Ubuntu Regular","Arial Regular","DejaVu Sans Book";
@white: #F0F8FF; /* blue-tinted, for Antarctica */
@red: #fdaf6b;
@orange: #fdc663;
@yellow: #fae364;
@green: #d3e46f;
@turquoise: #aadb78;
@blue: #a3cec9;
@purple: #ceb5cf;
@pink: #f3c1d3;
@f00: #f00;
@black:#FF0000;





#shaktiplots[zoom >= 15] {
  text-face-name:@font_reg;
  text-name:"[Id]";
  text-fill:@black;
  text-size: 9.5;
  text-halo-fill: fadeout(white, 30%);
  text-halo-radius: 2.5;
  text-placement-type: simple;

  text-dy: -13;
  marker-width:3;
  marker-fill:@red;
  marker-line-color:#813;
  marker-allow-overlap:true;
}




